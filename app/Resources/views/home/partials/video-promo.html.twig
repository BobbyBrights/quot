{% image '%kernel.root_dir%/Resources/public/images/machine_bg.png' %}
<div name="is_personal" id="is_personal" class='row' style="background-image:url({{ asset_url }}); background-position: center">
    <div class='row'>
        <div class='col-md-12'>
            <div class='is-personal-info'>
                <h3>{{ module.title }}</h3>
                <p>{{ module.field_descripcion }}</p>
            </div>
            <div class='is-personal-info-button'>
                <button onclick="window.location.href='{{ path('products') }}'">Personalizar</button>
            </div>
        </div>
    </div>
</div>
{% endimage %}

